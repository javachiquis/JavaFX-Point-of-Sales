<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="370.0" prefWidth="450.0" styleClass="vBox-background" stylesheets="@../styles/confirm.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" minWidth="-Infinity" prefHeight="45.0" prefWidth="450.0" VBox.vgrow="NEVER">
         <children>
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0" styleClass="close-button" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="250.0" prefWidth="450.0" VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="soldProducts" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="productColumn" prefWidth="107.0" text="Producto" />
                <TableColumn fx:id="productCost" prefWidth="95.0" text="Costo" />
                  <TableColumn fx:id="productQuantity" prefWidth="105.0" text="Cantidad" />
                  <TableColumn fx:id="productTotal" prefWidth="140.0" text="Total" />
              </columns>
            </TableView>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="100.0" prefWidth="450.0" VBox.vgrow="NEVER">
         <children>
            <Label fx:id="retailLabel" styleClass="retail" text="Retail: " />
            <Button mnemonicParsing="false" onAction="#doneAction" prefHeight="25.0" prefWidth="80.0" text="Hecho">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" />
   </padding>
</VBox>
